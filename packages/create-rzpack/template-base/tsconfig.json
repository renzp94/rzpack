{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM",  "ESNext"],
    "moduleResolution": "node",
    "allowJs": false,
    "skipLibCheck": false,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "noEmit": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "strict": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@/*":["./src/*"]
    }
  }
}